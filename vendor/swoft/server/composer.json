{
  "name": "swoft/server",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "swoft",
    "websocket-server",
    "websocket"
  ],
  "description": "base server for swoft",
  "license": "Apache-2.0",
  "require": {
    "swoft/stdlib": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Server\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Server\\Unit\\": "test/unit/",
      "SwoftTest\\Server\\Testing\\": "test/testing/"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.laravel-china.org"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
